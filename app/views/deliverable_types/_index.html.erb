
<table class="CatalogTable DisplayTable">
  <tr>
    <th id="HeaderLeft">Name</th>
    <th>Unit of measure</th>
    <th id="HeaderRight"></th>
  </tr>


  

<% @deliverable_types.each do |deliverable_type| %>
  <tr>
    <td><%= link_to h(deliverable_type.name), edit_deliverable_type_path(deliverable_type) %></td>
    <td><%=h deliverable_type.unit_of_measure %></td>
    <td><%= link_to 'X', deliverable_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
  <tr>
    <td colspan="3">
      <%= link_to image_tag("/images/plus_icon.png") + ' New Deliverable Type', new_deliverable_type_path %>
    </td>
  </tr>
</table>
