<table class="CatalogTable DisplayTable">
    <tr>
    <th id="HeaderLeft">Name</th>
    <th>Complexity</th>
    <th>Est. Size</th>
    <th>Est. Effort</th>
    <th>Est. Rate</th>
    <th>Type</th>
    <th>Phase</th>
    <th id="HeaderRight"></th>
  </tr>

<% @typical_deliverables.each do |typical_deliverable| %>
  <tr>
    <td><%= link_to h(typical_deliverable.name), edit_typical_deliverable_path(typical_deliverable) %></td>
    <td><%=  typical_deliverable.complexity %></td>
    <td><%=  typical_deliverable.estimated_size %></td>
    <td><%=  typical_deliverable.estimated_effort %></td>
    <td><%=  typical_deliverable.get_production_rate %> </td>
    <td><%= typical_deliverable.deliverable_type.name %></td>
    <td><%= typical_deliverable.lifecycle_phase.name %></td>
    <td><%= link_to 'x', typical_deliverable, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
  <tr>
    <td colspan="8">
<%= link_to image_tag("/images/plus_icon.png") + ' New Typical Deliverable', new_typical_deliverable_path %>
    </td>
  </tr>
</table>