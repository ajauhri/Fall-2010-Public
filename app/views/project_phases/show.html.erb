<table class="quickTable" id="quickPhase">

  <tr>
    <td><b>Name:</b></td>
    <td><%=h @project_phase.name %></td>
  </tr>

  <tr>
    <td> <b>Description:</b></td>
    <td> <%=h @project_phase.description %></td>
  </tr>

  <tr>
    <td> <b>Sequence:</b></td>
    <td><%=h @project_phase.sequence %></td>
  </tr>

  <tr>
    <td> <b>Total estimated effort:</b></td>
    <td> <%=h @project_phase.total_estimated_effort %></td>
  </tr>
  <tr>
    <td colspan="2">
      <%= link_to 'Edit', edit_project_phase_path(@project_phase) %> |
      <%= link_to 'Back', project_path(@project_phase.project_id) %>
    </td>
  </tr>
</table> 

